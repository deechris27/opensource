{
  "name": "mytidbit",
  "version": "1.0.0",
  "description": "hello world",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "eslint": "eslint .",
    "test": "nyc mocha",
    "test:watch": "mocha --watch",
    "validate": "npm-run-all --parallel eslint test build"
  },
  "nyc": {
    "check-coverage": true,
    "branches": 100,
    "functions": 100,
    "lines": 100,
    "statements": 100
  },
  "author": "Deepak <dd43030@gmail.com> (http://tidbit.c.la/)",
  "license": "MIT",
  "dependencies": {
    "@babel/cli": "*",
    "@babel/preset-env": "*",
    "@babel/register": "7.9.0",
    "babel-loader": "8.1.0",
    "babel-plugin-istanbul": "6.0.0",
    "cross-env": "7.0.2",
    "eslint-config-airbnb": "18.1.0",
    "json-loader": "0.5.7",
    "unique-random-array": "2.0.0",
    "webpack": "4.43.0"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "ghooks": "2.0.4",
    "mocha": "7.2.0",
    "npm-run-all": "4.1.5",
    "nyc": "15.0.1",
    "rimraf": "3.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run validate"
    }
  }
}
